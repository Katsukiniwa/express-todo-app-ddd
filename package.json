  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/app.ts",
    "lint-fix": "eslint --fix 'src/**/*.ts' && prettier --write 'src/**/*.ts'",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
